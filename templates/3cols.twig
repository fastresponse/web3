{% extends 'base.twig' %}

{% set css = ['3cols.css']|merge( css|default( [] ) ) %}

{% block main %}
  <div id="left-col">
    {% block left %}
    {{ left|raw }}
    {% endblock left %}
  </div>
  <div id="center-col">
    {% block center %}
    {{ center|raw }}
    {% endblock center %}
  </div>
  <div id="right-col">
    {% block right %}
    {{ right|raw }}
    {% endblock right %}
  </div>
{% endblock main %}

